<%- include ('../../partials/header.ejs') %>

<%- include ('../../partials/navbar.ejs') %>

<body>
    <div class="container">
        <hr>
        <h2>Users</h2>
        <a href="/admin/users/create" class="btn btn-success">New User</a>
        <hr>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Email</th>
                </tr>
            </thead>
            <% users.forEach(user=>{ %>
                <tr>
                    <th><%=user.id %></th>
                    <th><%=user.email %></th>
                </tr>
            <% }) %>
            
        </table>
    </div>
</body>

<%- include ('../../partials/footer.ejs') %>